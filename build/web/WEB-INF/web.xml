<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>loginserv</servlet-name>
        <servlet-class>test.com.loginserv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addnewservlet</servlet-name>
        <servlet-class>addnewservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listuprj</servlet-name>
        <servlet-class>test.com.listuprj</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>verwrk</servlet-name>
        <servlet-class>test.com.verwrk</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cityrep</servlet-name>
        <servlet-class>test.com.cityrep</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginserv</servlet-name>
        <url-pattern>/loginserv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addnewservlet</servlet-name>
        <url-pattern>/addnewservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listuprj</servlet-name>
        <url-pattern>/listuprj</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>verwrk</servlet-name>
        <url-pattern>/verwrk</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cityrep</servlet-name>
        <url-pattern>/cityrep</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
